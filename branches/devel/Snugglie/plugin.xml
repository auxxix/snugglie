<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension-point id="com.snugglie.exceptionhandler" name="exceptionhandler" schema="schema/com.snugglie.exceptionhandler.exsd"/>
   <extension-point id="com.snugglie.packethandler" name="packethandler" schema="schema/com.snugglie.packethandler.exsd"/>
   <extension
         point="org.eclipse.ui.console.consoleFactories">
      <consoleFactory
            class="com.snugglie.Activator"
            icon="icons/snugglie.png"
            label="Snugglie">
